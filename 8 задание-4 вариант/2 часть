# Процедура для проверки, лежит ли точка внутри окружности
def is_point_inside_circle(x, y, a, b, R):
    # Проверка, лежит ли точка внутри окружности
    return (x - a)**2 + (y - b)**2 < R**2

# Основная программа
def count_points_inside_circle(a, b, R, points):
    count = 0
    for point in points:
        # Проверяем, лежит ли точка внутри окружности
        if is_point_inside_circle(point[0], point[1], a, b, R):
            count += 1
    return count

# Заданные данные
a = 0  # Координата центра окружности (a)
b = 0  # Координата центра окружности (b)
R = 5  # Радиус окружности

# Точки, которые будем проверять
points = [
    (1, 1),  # Точка P
    (3, 4),  # Точка F
    (6, 6),  # Точка L
    (0, 0)   # Еще одна точка для примера
]

# Выводим, сколько точек лежит внутри окружности
count = count_points_inside_circle(a, b, R, points)
print(f"Количество точек, лежащих внутри окружности: {count}")
